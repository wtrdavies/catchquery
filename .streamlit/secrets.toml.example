# Streamlit Secrets Template
# Copy this file to secrets.toml and fill in your actual values
# secrets.toml is gitignored and will never be committed

# OpenRouter API Key
# Get yours at: https://openrouter.ai/keys
OPENROUTER_API_KEY = "your-api-key-here"

# Application Password
# Choose a secure password for your deployment
APP_PASSWORD = "your-password-here"

# Optional: Disable authentication for local testing
# Set to true to skip password prompt when developing locally
# IMPORTANT: Remove this line or set to false before deploying to production!
DISABLE_AUTH_FOR_LOCAL_TESTING = false
